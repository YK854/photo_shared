<div class="h3 mt-5">
  <%= photo.title %>
</div>
<div class="h6 text-center py-3 border-bottom">
  <%= photo.description %>
</div>
<div class="border-bottom">
  <% photo.tag_list.each do |tag| %>
  <div class="btn btn-outline-info my-2 p-0 px-2" disabled>
    <%= tag %>
  </div>
  <% end %>
</div>
